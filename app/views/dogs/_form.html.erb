<%= simple_form_for(@dog) do |f| %>
  <%= f.input :name %>
  <%= f.input :breed %>
  <%= f.input :description %>
  <%= f.input :city %>
  <%= f.input :age %>
  <%= f.input :palmares %>
  <%= f.input :price_per_hour %>
  <%= f.input :profile_picture,as: :file %>
  <%= f.input :pictures,as: :file, input_html: {multiple: true } %>
  <%= f.button :submit, class: "btn btn-outline-primary rounded-pill" %>
<% end %>
